<body>

		 

<div id="demo"></div>









<script>
		
const mA = math.matrix([

	[1, 2], 

	[3, 4], 
	
	[5, 6]
]);



const matrixMult = math.multiply(2, mA);

console.log(matrixMult);

document.getElementById('demo').innerHTML = matrixMult;


</script>





</body>




