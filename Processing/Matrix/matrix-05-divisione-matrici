<body>

		 

<div id="demo"></div>









<script>
	
const mA = math.matrix([

	[0, 2], 

	[4, 6], 
	
	[8, 10]
]);



const matrixDiv = math.divide(mA, 2);

console.log(matrixDiv);

document.getElementById('demo').innerHTML = matrixDiv;


</script>





